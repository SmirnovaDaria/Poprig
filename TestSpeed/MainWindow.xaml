<Window x:Class="TestSpeed.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestSpeed"
        mc:Ignorable="d"
        Title="Попрыжёнок" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="380*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0">
            <Image Source="/Resources/Попрыженок.png" Margin="10"/>
            <TextBlock Text="Попрыжёнок" VerticalAlignment="Center" HorizontalAlignment="Left" Style="{StaticResource TitleTextBlock}"/>
        </DockPanel>
        <Frame Grid.Row="1" x:Name="FramePage" NavigationUIVisibility="Hidden">

        </Frame>
    </Grid>
</Window>
